 "html": "<div class=\"page-break\">\n    <div id=\"header-html\" class=\"hidden-pdf\">\n        {%- if letter_head and not no_letterhead -%}\n            <div class=\"letter-head\">\n                {{ letter_head }}\n            </div>\n        {%- endif -%}\n    </div>\n\n\n    <div id=\"footer-html\" class=\"visible-pdf\">\n        {% if not no_letterhead and footer %}\n            <div class=\"letter-head-footer\">\n            {{ footer }}\n            </div>\n        {% endif %}\n\n        <p class=\"text-center small page-number visible-pdf\">\n            {{ _(\"Page {0} of {1}\").format('<span class=\"page\"></span>', '<span class=\"topage\"></span>') }}\n        </p>\n    </div>\n\n    <div class=\"print-heading\">\n        <div  style=\"text-align: center;\">\n            <h3>MATERIAL RECEIPT</h3>\n            <h5><u>\n                {{ doc.branch }}\n            </u></h5>\n\n        </div>\n    </div>\n\n    <div class=\"row section-break\">\n\t    <div class=\"col-xs-12 column-break\">\n            <table class = \"table1\" style=\"margin-top: 1px; text-align:justify;font-size:small; width:100% \">\n\n                <tr>\n                    <td><strong>MR No.:</strong></td>\n                    <td>{{ doc.name }}</td>\n                    <td><strong>MR Date:</strong></td>\n                    <td>{{ frappe.utils.formatdate(doc.posting_date) }}</td>\n                </tr>\n                <tr>\n                    <td><strong>Dispatch Date: </strong></td>\n                    <td> {{ doc.vehicle_dispatch_date_and_time }} </td>\n                    <td><strong>Transporter Name: </strong></td> <td> {{ doc.transporter_name }} </td>\n                </tr>\n                <tr>\n                    <td><strong>Vehicle No:</strong></td>\n                    <td>{{ doc.vehicle_no }}</td>\n                    <td><strong>MR Title:</strong></td>\n                    <td>{{ doc.title }}</td>\n                </tr>\n            </table>\n        </div>\n    </div>\n\n    <div class=\"row section-break\">\n\t    <div class=\"col-xs-12 column-break\">\n            <table class=\"table table-bordered\" style=\"margin-top: 15px;\">\n                <tbody>\n                    <tr>\n                        <th>Sr</th>\n                        <th style=\"text-align: center;\">Material Code</th>\n                        <th style=\"text-align: center;\">Material Name</th>\n                        <th style=\"text-align: center;\">UoM</th>\n                        <th style=\"text-align: center;\">Qty Issued</th>\n                        <th style=\"text-align: center;\">Rate</th>\n                        <th class=\"text-right\"  style=\"text-align: center;\">Amount</th>\n                    </tr>\n                    {%- for row in doc.items -%}\n                    <tr>\n                        <td style=\"width: 3%;\">{{ row.idx }}</td>\n                        <td style=\"width: 10%;\">{{ row.item_code }}</td>\n                        <td style=\"width: 45%;text-align:left;\">{{ row.item_name }}{%- if frappe.db.get_value(\"Item Group\", row.item_group, \"show_subgroup_in_print\") -%}\n                        {%- if frappe.db.get_value(\"Item\", {\"name\": row.item_code}, \"item_sub_group\") -%}\n                        <br/><i><small>({{frappe.db.get_value(\"Item\", {\"name\": row.item_code}, \"item_sub_group\")}})</i>\n                        {%- endif -%}\n                        {%- endif -%}</td>\n                        <td style=\"width: 10%;\">{{ row.uom }}</td>\n                        <td style=\"width: 10%; text-align:right;\">{{ row.qty }}</td>\n                        <td style=\"width: 10%; text-align:right;\">{{ row.valuation_rate }}</td>\n                        <td style=\"width: 12%; text-align:right;\">{{ row.amount }}</td>\n                    </tr>\n                    {%- endfor -%}\n                    <tr><td colspan= \"6\" style=\"text-align:right;\"><b>Total Amount:</b></td><td style= \"text-align:right;\">{{ doc.total_amount }}</td></tr>\n                </tbody>\n            </table>\n        </div>\n    </div>\n\n    <div style=\"float: left; width: 100%; font-size: small\">\n        <strong>In Words: </strong>Ngultrum {{ (frappe.utils.money_in_words(doc.total_amount))[3:] }} \n    </div>\n\t\n    <div class=\"row section-break\" style=\"padding-top: 80px; clear: both;\">\n\t    <div class=\"col-xs-12 column-break\">\n            <div class=\"row data-field\">\n                <div class=\"col-xs-12\">Authorized Signatory</div>\n            </div>\n            <div class=\"row data-field\" style=\"padding-top: 40px;\">\n                <div class=\"col-xs-6\">(Issued By)</div>\n                <div class=\"col-xs-6\">(Received by)</div>\n            </div>\n        </div>\n    </div>\n    <!-- <div style=\"padding-top: 100px; clear: both;\">\n        <div>Authorized Signatory</div>\n        <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Issued By) </div>\n        <div style=\"padding-top: 80px; width: 33%; float: left; text-align:center;\">(Carrier) </div>\n        <div style=\"padding-top: 80px; width: 33%; float: right; text-align:center;\">(Received by) </div>\n    </div> -->\n    \n    <div style=\"padding-top: 100px; clear: both;\">\n        <div class=\"printedby\">This print is system generated and should be considered VALID only with appropriate seal and/or signature. <br>  Printed by {{ frappe.user }} on {{ frappe.utils.nowdate()  }} at {{ frappe.utils.nowtime() }}</div>\n    </div>\n</div>", 
